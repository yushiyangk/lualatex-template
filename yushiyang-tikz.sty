\ProvidesPackage{yushiyang-tikz}

% Provide general vector graphics (basis of most embedded graphics packages)
\usepackage{tikz}

% Improve TikZ compatibility with package babel
\usetikzlibrary{babel}

% Externalise TikZ graphics to reduce recompilation (see TikZ manual)
% Requires --shell-escape option
\usetikzlibrary{external}
\tikzexternalize
\tikzsetexternalprefix{tikz/}
